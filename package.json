{
  "name": "chaibuilder-multi-tenant",
  "version": "0.1.22",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npx chai-tailwind -c ./tailwind.site.config.ts -i ./app/\\(sites\\)/site.css --watch\" \"next dev --turbopack --port 3000\"",
    "dev:turbo": "concurrently \"npx chai-tailwind -c ./tailwind.site.config.ts -i ./app/\\(sites\\)/site.css --watch\" \"next dev --turbopack --port 3000\"",
    "build": "npx chai-tailwind -c ./tailwind.site.config.ts -i ./app/\\(sites\\)/site.css && next build",
    "start": "next start",
    "lint": "next lint & tsc --noEmit",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "check-env": "node scripts/check-env.cjs"
  },
  "dependencies": {
    "@next/third-parties": "^15.5.0",
    "@openfeature/react-sdk": "^1.0.1",
    "@openfeature/server-sdk": "^1.19.0",
    "@paddle/paddle-js": "^1.4.2",
    "@paddle/paddle-node-sdk": "^3.2.1",
    "@radix-ui/react-aspect-ratio": "^1.1.7",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-radio-group": "^1.3.8",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-toggle-group": "^1.1.11",
    "@tanstack/react-query": "^5.85.9",
    "@vercel/analytics": "^1.5.0",
    "@vercel/sdk": "^1.10.5",
    "ai": "^5.0.59",
    "botid": "^1.5.6",
    "chai-next": "0.1.10",
    "cheerio": "^1.1.0",
    "dodopayments": "^2.1.1",
    "dodopayments-checkout": "^1.3.1",
    "embla-carousel-react": "^8.6.0",
    "framer-motion": "^12.23.24",
    "input-otp": "^1.4.2",
    "lodash": "^4.17.21",
    "lucide-react": "^0.544.0",
    "motion": "^12.23.24",
    "nanoid": "^5.1.6",
    "next": "15.5.6",
    "next-themes": "^0.4.6",
    "react": "19.1.0",
    "react-diff-view": "^3.3.2",
    "react-dom": "19.1.0",
    "react-error-boundary": "^6.0.0",
    "react-hook-form": "^7.62.0",
    "react-resizable-panels": "^3.0.4",
    "recharts": "^3.1.2",
    "sharp": "^0.34.4",
    "vanilla-cookieconsent": "^3.1.0",
    "vaul": "^1.1.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/container-queries": "^0.1.1",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "@types/lodash": "^4.17.20",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.1.2",
    "eslint": "^8",
    "eslint-config-next": "^15.4",
    "fs-extra": "^11.3.0",
    "postcss": "^8.4.49",
    "prettier": "^3.4.2",
    "tailwindcss": "^3.4.13",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5"
  },
  "pnpm": {
    "overrides": {
      "@types/react": "19.0.8",
      "@types/react-dom": "19.0.3",
      "sharp": "$sharp"
    }
  },
  "overrides": {
    "sharp": "$sharp"
  }
}
